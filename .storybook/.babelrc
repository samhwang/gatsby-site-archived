{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    [
      "babel-preset-react-app",
      {
        "flow": false,
        "typescript": true
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-remove-graphql-queries"
  ]
}